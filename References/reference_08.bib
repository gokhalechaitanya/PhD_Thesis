	Pages = {1675-1688},
	Pmc = {PMC1207430},
	Pmid = {8844155},
	Pst = {ppublish},
	Title = {Rapid evolution of a coadapted gene complex: evidence from the Segregation Distorter (SD) system of meiotic drive in Drosophila melanogaster},
