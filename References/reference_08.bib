	Mesh = {Adolescent; Adult; Aged; Anticoagulants; Blood Cell Count; Bone Marrow Cells; Bone Marrow Transplantation; Cause of Death; Chronic Disease; Female; Hemoglobinuria, Paroxysmal; Humans; Leukemia; Male; Middle Aged; Platelet Transfusion; Remission, Spontaneous; Survival Analysis; Thrombophlebitis},
	Pages = {1253-1258},
